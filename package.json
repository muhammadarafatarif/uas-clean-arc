{
   "name": "uas-clean-arc",
   "version": "1.0.0",
   "description": "User Authentication System in NodeJS using Clean Architecture with scaffold package",
   "scripts": {
      "start": "node ./dist/index.js",
      "build": "rimraf dist && tsc -p tsconfig-build.json",
      "watch": "nodemon --exec \"npm run build && npm run start\" --watch src --ext ts"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/muhammadarafatarif/uas-clean-arc.git"
   },
   "keywords": [
      "nodejs",
      "typescript",
      "clean-architecture",
      "express",
      "mongodb",
      "JWT",
      "REST",
      "APIs",
      "tsclean",
      "scaffold"
   ],
   "author": "Muhammad Arafat Arif",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/muhammadarafatarif/uas-clean-arc/issues"
   },
   "homepage": "https://github.com/muhammadarafatarif/uas-clean-arc#readme",
   "dependencies": {
      "@tsclean/core": "^1.7.0",
      "dotenv": "^10.0.0",
      "helmet": "^4.6.0",
      "module-alias": "^2.2.2",
      "mongoose": "^6.0.10"
   },
   "devDependencies": {
      "@types/node": "^16.9.1",
      "@types/jest": "^27.0.1",
      "nodemon": "^2.0.9",
      "rimraf": "^3.0.2",
      "ts-jest": "^27.0.5",
      "ts-node": "^10.2.1",
      "typescript": "^4.4.3",
      "@types/mongoose": "^5.11.97"
   },
   "_moduleAliases": {
      "@": "dist"
   }
}